@article{svenssonMechanicalPropertiesHuman2012,
  title = {Mechanical Properties of Human Patellar Tendon at the Hierarchical Levels of Tendon and Fibril},
  author = {Svensson, René B. and Hansen, Philip and Hassenkam, Tue and Haraldsson, Bjarki T. and Aagaard, Per and Kovanen, Vuokko and Krogsgaard, Michael and Kjaer, Michael and Magnusson, S. Peter},
  year = 2012,
  month = feb,
  journal = {Journal of Applied Physiology},
  volume = {112},
