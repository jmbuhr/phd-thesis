@article{hartmannMechanismCollagenFolding2021,
  title = {Mechanism of Collagen Folding Propagation Studied by {{Molecular Dynamics}} Simulations},
  author = {Hartmann, Julian and Zacharias, Martin},
  year = 2021,
  month = jun,
  journal = {PLOS Computational Biology},
  volume = {17},
  number = {6},
  pages = {e1009079},
